	author = {Yang, X. and Post, W. M. and Thornton, P. E. and Jain, A.},
	month = apr,
	year = {2013},
	pages = {2525--2537},
	file = {Full Text PDF:/Users/fvrodriguez/Zotero/storage/BBPNDWCP/Yang et al. - 2013 - The distribution of soil phosphorus for global bio.pdf:application/pdf;Snapshot:/Users/fvrodriguez/Zotero/storage/XZSUVXLT/2013.html:text/html}
}

