	file = {Pyhäjärvi et al. - 2013 - Complex Patterns of Local Adaptation in Teosinte.pdf:/Users/fvrodriguez/Zotero/storage/NWKFK7QA/Pyhäjärvi et al. - 2013 - Complex Patterns of Local Adaptation in Teosinte.pdf:application/pdf}
}

